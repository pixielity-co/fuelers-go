{
  "name": "@packages/<%= name %>",
  "version": "1.0.0",
  "private": true,
  "description": "<%= name %> package",
  "main": "src/<%= name %>.go",
  "scripts": {
    "setup": "echo 'Setting up <%= name %> package...'",
    "dev": "air",
    "build": "go build ./src/... && mkdir -p dist && touch dist/.built",
    "test": "go test -v ./src/...",
    "lint": "golangci-lint run ./src/...",
    "clean": "rm -rf dist coverage.out",
    "typecheck": "go vet ./src/...",
    "tidy": "go mod tidy",
    "format": "go fmt ./src/..."
  },
  "dependencies": {},
  "devDependencies": {},
  "peerDependencies": {}
}
